{
  "buildCommand": "npm run build:examples",
  "functions": {
    "api/ssr.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/ssr"
    }
  ]
}
