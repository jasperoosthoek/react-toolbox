{
  "buildCommand": "npm run build:examples",
  "functions": {
    "api/ssr.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/favicon.png",
      "destination": "/favicon.png"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.png"
    },
    {
      "source": "/:file*.(png|jpg|jpeg|gif|webp|svg|ico|css|js|json|txt)",
      "destination": "/:file*"
    },
    {
      "source": "/:path*",
      "destination": "/api/ssr"
    }
  ],
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/:file*.(png|jpg|jpeg|gif|webp|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/:file*.(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}