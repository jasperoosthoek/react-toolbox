{
  "buildCommand": "npm run build:examples",
  "outputDirectory": "dist-examples",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/favicon.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css))",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "public, max-age=31536000"
        }
      ]
    }
  ]
}
