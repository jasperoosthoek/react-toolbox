{
  "name": "@jasperoosthoek/react-toolbox",
  "version": "0.1.1",
  "author": "jasperoosthoek",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jasperoosthoek/react-toolbox.git"
  },
  "description": "Functional factory to create Redux actions and reducers that interface with a backend",
  "files": [
    "src/",
    "lib-cjs/",
    "lib-esm/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.16.7",
    "@babel/register": "^7.13.16"
  },
  "main": "./lib-cjs/index.js",
  "module": "./lib-esm/index.js",
  "scripts": {
    "_build": "babel src --extensions '.js' --source-maps=true",
    "build:cjs": "NODE_ENV=production BABEL_ENV=cjs npm run _build -- --out-dir lib-cjs --delete-dir-on-start",
    "build:esm": "NODE_ENV=production BABEL_ENV=esm npm run _build -- --out-dir lib-esm --delete-dir-on-start",
    "prepublishOnly": "npm run build:esm && npm run build:cjs"
  },
  "bugs": {
    "url": "https://github.com/jasperoosthoek/react-toolbox/issues"
  },
  "homepage": "https://github.com/jasperoosthoek/react-toolbox#readme",
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "bootstrap": "^5.1.3",
    "react-bootstrap": "^2.0.4",
    "react-icons": "^4.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "keywords": [
    "react",
    "ui"
  ]
}
